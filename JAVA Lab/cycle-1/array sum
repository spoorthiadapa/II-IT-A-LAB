import java.util.*;
import java.lang.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    System.out.print("enter the size: ");
	    int n = sc.nextInt();
	    int[] a = new int[n];
	    int sum=0;
	    System.out.println("enter array elements");
	    for(int i=0;i<n;i++){
	        a[i]=sc.nextInt();
	        sum+=a[i];
	    }
	     System.out.println("sum of array elements : "+sum); 
	}
}
